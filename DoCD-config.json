{
  "ProjectName": "test",
  "BasePackageManager": "choco",
  "InstallServices": true,
  "Services": [
    {
      "ServiceName": "nodejs",
      "PackageManager": "npm",
      "Path": "./client",
      "LogFilePath": "./logs/react-service.log",
      "InstallationCommands": [
        {
          "Directory": "\\client",
          "Command": "npm install"
        }
      ],
      "BuildCommands": [
        {
          "Directory": "\\client",
          "Command": "npm run start"
        }
      ]
    }
  ]
}
